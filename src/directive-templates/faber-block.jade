label
  input(type="checkbox", ng-model="expanded")
  | {{$id}}
  button(ng-click="removeSelf()") x

div(ng-if="expanded")
  faber-component-renderer

  div(ng-if="component.type=='group'")
    faber-components
    ol.faber-blocks
      li.faber-block(ng-repeat="data in block.blocks")
        faber-block(data-faber-block-content="data")